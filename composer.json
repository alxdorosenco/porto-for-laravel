{
    "name": "alxdorosenco/porto-for-laravel",
    "description": "This package is a solution for easy building and working with Porto pattern structure",
    "keywords": [
        "php",
        "laravel",
        "laravel-porto",
        "laravel-porto-console",
        "porto-for-laravel",
        "porto",
        "template",
        "pattern",
        "architecture"
    ],
    "homepage": "https://github.com/alxdorosenco/porto-for-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexei Dorosenco",
            "email": "alexforever1808@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "AlxDorosenco\\PortoForLaravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AlxDorosenco\\PortoForLaravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AlxDorosenco\\PortoForLaravel\\PortoServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
